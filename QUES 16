#include <iostream>
using namespace std;
int main()
{
    int n;
    cout << "Enter a number:";
    cin >> n;
    if (n <= 1)
    {
        cout << "given number is not a prime number";
    }
    int i;
    for (i = 2; i <= n / 2; i++)
    {
        if (n % i == 0)
        {
            cout << "Given number is not a prime number";
            return 0;
        }
    }
    cout << "given number is a prime number";

    return 0;
}
