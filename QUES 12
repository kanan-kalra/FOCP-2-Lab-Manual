#include <iostream>
using namespace std;
int main()
{
    int a, b, c;
    cout << "Enter the length of first side of the triangle:";
    cin >> a;
    cout << "Enter the length of second side of the triangle:";
    cin >> b;
    cout << "Enter the length of third side of the triangle:";
    cin >> c;
    if (a == b && b == c)
    {
        cout << "it's an equilateral triangle";
    }
    else if (a == b)
    {
        cout << "it's an isosceles triangle";
    }
    else
    {
        cout << "it's an scalene triangle";
    }
    return 0;
}
