#include <iostream>
#include <string.h>
using namespace std;
int main()
{
    string id;
    cout << "Enter the ID: ";
    cin >> id;

    int i = 0;
    int j = id.length() - 1;

    while (i < j)
    {
        if (id[i] != id[j])
        {
            cout << "Given ID is not a palindrome";
            return 0;
        }
        i++;
        j--;
    }

    cout << "Given ID is a palindrome";
    return 0;
}
