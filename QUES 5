#include <iostream>
using namespace std;
int main()
{
    int a;
    cout << "enter the value of first number:";
    cin >> a;
    int b;
    cout << "enter the value of second number:";
    cin >> b;
    a = a + b;
    b = a - b;
    a = a - b;
    cout << "first value after switching:" << a;
    cout << endl
         << "second value after switching is:" << b;
    return 0;
}
